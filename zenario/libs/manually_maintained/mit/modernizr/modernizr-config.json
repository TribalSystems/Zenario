{
  "classPrefix": "",
  "enableClasses": true,
  "enableJSClass": true,
  "scriptGlobalName": "window",
  "usePrefixes": true,
  "minify": false,
  "options": [
    "addTest",
    "domPrefixes",
    "hasEvent",
    "prefixed",
    "prefixes",
    "setClasses",
    "testAllProps",
    "testProp",
    "testStyles"
  ],
  "feature-detects": [
    "applicationcache",
    "audio",
    "blob",
    "canvas",
    "canvastext",
    "css/animations",
    "css/columns",
    "css/gradients",
    "css/reflections",
    "css/transforms",
    "css/transforms3d",
    "css/transitions",
    "geolocation",
    "hashchange",
    "history",
    "indexeddb",
    "input",
    "inputtypes",
    "postmessage",
    "storage/localstorage",
    "storage/sessionstorage",
    "storage/websqldatabase",
    "svg",
    "svg/clippaths",
    "svg/smil",
    "video",
    "webgl",
    "websockets",
    "workers/webworkers"
  ]

}
