<?xml version="1.0" encoding="UTF-8"?>
<phpunit backupGlobals="false"
         backupStaticAttributes="false"
         colors="true"
         convertErrorsToExceptions="true"
         convertNoticesToExceptions="true"
         convertWarningsToExceptions="true"
         processIsolation="false"
         stopOnFailure="false"
         syntaxCheck="false"
         bootstrap="./test/NumbersPHP/bootstrap.php"
        >
    <testsuites>
        <testsuite name="NumbersPHP Test Suite">
            <directory suffix="Test.php">./test/NumbersPHP/Tests</directory>
        </testsuite>
    </testsuites>
</phpunit>