{
	"name": "tribalsystems/zenario",
	"type": "project",
	"description": "Zenario is a web-based content management system for sites with one or many languages.",
	"keywords": ["cms", "content management", "wysiwyg", "zenario"],
	"homepage": "https://github.com/TribalSystems/Zenario",
	"license": "MIT",
	"require": {
		"php": ">=7.0",
		"mustangostang/spyc": "*",
		"phpmailer/phpmailer": "~6.0",
		"powder96/numbers.php": "@dev",
		"ps/image-optimizer": "*",
		"twig/twig": "^2.0"
	},
	"require-dist": {
		"php": ">=7.0",
		"mustangostang/spyc": "*",
		"phpmailer/phpmailer": "~6.0",
		"powder96/numbers.php": "@dev",
		"ps/image-optimizer": "*",
		"twig/twig": "^2.0"
	},
	"require-no_dist": {
		"php": ">=7.0",
		"cboden/ratchet": "*",
		"defuse/php-encryption": "^2.0",
		"mongodb/mongodb": "^1.0.0",
		"mustangostang/spyc": "*",
		"php-amqplib/php-amqplib": "2.5.*",
		"phpmailer/phpmailer": "~6.0",
		"powder96/numbers.php": "@dev",
		"ps/image-optimizer": "*",
		"textalk/websocket": "*",
		"twig/twig": "^2.0",
		"gocardless/gocardless-pro": "^1.1"
	},
	"require-removed": {
		"duoshuo/php-cassandra": "dev-master",
		"kachkaev/php-r": "dev-master"
	},
	"autoload": {
		"psr-4": { "ze\\": "zenario/autoload" }
	},
	"config": {
		"vendor-dir": "zenario/libs/composer_dist",
		"vendor-dir-dist": "zenario/libs/composer_dist",
		"vendor-dir-no_dist": "zenario/libs/composer_no_dist",
		"optimize-autoloader": true,
		"classmap-authoritative": true
	}
}